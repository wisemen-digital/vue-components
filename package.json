{
  "name": "wisemen-components",
  "version": "0.0.4",
  "scripts": {
    "build:components": "ts-node --experimental-specifier-resolution=node --project ./tsconfig.scripts.json ./src/scripts/generateRegistry.ts",
    "dev": "vite",
    "build": "pnpm build:components && vite build",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "typecheck-watch": "vue-tsc --noEmit --skipLibCheck --watch",
    "serve": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "production": "vite build --mode production",
    "staging": "vite build --mode staging",
    "development": "vite build --mode development",
    "story:dev": "histoire dev",
    "story:build": "pnpm build:components && histoire build",
    "story:preview": "histoire preview",
    "test": "vitest ",
    "coverage": "vitest run --coverage",
    "color-generator:dev": "vite",
    "precommit": "pnpm lint && pnpm typecheck && pnpm story:build && pnpm build:components"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@floating-ui/dom": "^1.5.3",
    "@headlessui-float/vue": "^0.13.0",
    "@headlessui/vue": "0.0.0-insiders.f2179f3",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@tanstack/vue-query": "^5.17.0",
    "@tanstack/vue-table": "^8.11.2",
    "@vueuse/core": "^10.7.1",
    "@vueuse/head": "^2.0.0",
    "@vueuse/router": "^10.7.1",
    "axios": "^1.6.3",
    "cva": "1.0.0-beta.1",
    "jsdom": "^23.0.1",
    "nodemon": "^3.0.2",
    "pinia": "^2.1.7",
    "sass": "^1.69.6",
    "scss": "^0.2.4",
    "tailwind-merge": "^2.2.0",
    "ts-node": "^10.9.2",
    "vue": "^3.4.3",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@changesets/cli": "^2.27.1",
    "@histoire/plugin-vue": "^0.17.6",
    "@testing-library/vue": "^8.0.1",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "autoprefixer": "^10.4.16",
    "eslint": "8.56",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "histoire": "^0.17.6",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "unplugin-turbo-console": "^1.0.9",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  }
}
