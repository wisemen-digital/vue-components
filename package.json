{
  "name": "wisemen-components",
  "version": "0.0.0",
  "scripts": {
    "build:components": "ts-node --experimental-specifier-resolution=node --project ./tsconfig.scripts.json ./src/scripts/generateRegistry.ts",
    "dev": "vite",
    "build": "pnpm lint && pnpm ts-check && pnpm build:components && vite build",
    "ts-check": "vue-tsc --noEmit --skipLibCheck",
    "ts-watch": "vue-tsc --noEmit --skipLibCheck --watch",
    "serve": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "production": "vite build --mode production",
    "staging": "vite build --mode staging",
    "development": "vite build --mode development",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "color-generator:dev": "vite color-generator"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@headlessui-float/vue": "^0.11.2",
    "@headlessui/vue": "^1.7.16",
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@tanstack/vue-query": "^4.34.0",
    "@tanstack/vue-table": "^8.9.3",
    "@vueuse/core": "^10.4.0",
    "@vueuse/head": "^1.3.1",
    "axios": "^1.4.0",
    "cva": "1.0.0-beta.1",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "pinia": "^2.1.6",
    "sass": "^1.66.1",
    "scss": "^0.2.4",
    "tailwind-merge": "^1.14.0",
    "ts-node": "^10.9.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@histoire/plugin-vue": "^0.17.1",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@vitejs/plugin-vue": "^4.3.3",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "histoire": "^0.17.0",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  }
}